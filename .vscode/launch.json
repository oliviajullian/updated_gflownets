    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--batch_size","256",
                "--lr", "1e-4",
                "--params_num_samples", "64",
                "--model", "mlp_gauss",
                "--artifact",  "dag_gflownet/data/sim_data"  

            ]
        }
    ]
}