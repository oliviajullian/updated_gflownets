    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--num_envs","1",
                "--num_variables", "5",
                "--num_edges_per_node", "5",
                "--num_edges", "5",




                "--model", "mlp_gauss",


                "--lr", "1e-4",
                "--batch_size","64",
                "--num_iterations","100",
                "--params_num_samples", "1000",
                "--update_target_every","10",

                "--replay_capacity","100_000_000",
                "--replay_capacity","100000",

                "--out_dir","dag_gflownet/data/results",
            ]
        }
    ]
}